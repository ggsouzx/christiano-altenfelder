{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/editor.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/blog.html"
    }
  ]
}